{
    "name": "vanilo/framework",
    "description": "E-commerce Framework for Laravel",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/vanilophp/framework/issues"
    },
    "require": {
        "php": "^7.2",
        "laravel/framework": "~6.0|~7.0",
        "konekt/appshell": "2.0.x-dev",
        "spatie/laravel-medialibrary": "^7.3",
        "vanilo/contracts": "@dev",
        "vanilo/support": "@dev",
        "vanilo/category": "2.0.x-dev",
        "vanilo/product": "2.0.x-dev",
        "vanilo/cart": "2.0.x-dev",
        "vanilo/checkout": "@dev",
        "vanilo/order": "2.0.x-dev",
        "vanilo/properties": "2.0.x-dev",
        "vanilo/channel": "2.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Vanilo\\Framework\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "require-dev": {
        "orchestra/testbench": "~4.0|~5.0",
        "phpunit/phpunit" : "~8.0|~9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Framework\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
